  "devDependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.2",
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.7",
    "resolve-url-loader": "2.3.1",
    "sass": "^1.16.0",
    "sass-loader": "7.*",
    "vue": "^2.5.22",
    "vue-localstorage": "^0.6.2",
    "vue-template-compiler": "^2.5.22",
    "chart.js": "^2.7.3"
  },
  "dependencies": {
    "vue-router": "^3.0.2"
